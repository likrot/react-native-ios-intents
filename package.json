{
  "name": "react-native-ios-intents",
  "version": "0.1.0",
  "description": "React Native library for iOS Siri Shortcuts, Live Activities, and App Intents",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "bin": {
    "react-native-ios-intents": "scripts/cli.js"
  },
  "exports": {
    ".": {
      "source": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./mock": {
      "source": "./src/__mocks__/index.ts",
      "types": "./src/__mocks__/index.ts",
      "default": "./src/__mocks__/index.ts"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "src",
    "lib",
    "ios",
    "cpp",
    "nitrogen",
    "scripts",
    "nitro.json",
    "*.podspec",
    "react-native.config.js",
    "!ios/build",
    "!ios/GeneratedAppIntents.swift",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/.*"
  ],
  "scripts": {
    "example": "npm run --prefix example",
    "test": "jest",
    "typecheck": "tsc",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "clean": "del-cli example/ios/build lib scripts/cli",
    "build:cli": "tsc src/cli/generate-shortcuts.ts --outDir scripts --moduleResolution node --esModuleInterop --target ES2020 --module commonjs --declaration --skipLibCheck",
    "prepare": "npm run build:cli && bob build",
    "nitrogen": "nitrogen",
    "generate-shortcuts": "node scripts/cli/generate-shortcuts.js",
    "generate-shortcuts:example": "node scripts/cli/generate-shortcuts.js --example",
    "release": "release-it --only-version"
  },
  "keywords": [
    "react-native",
    "ios",
    "siri",
    "shortcuts",
    "app-intents",
    "live-activities"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likrot/react-native-ios-intents.git"
  },
  "dependencies": {
    "commander": "^12.0.0"
  },
  "author": "likrot <likrot@eblank.eu> (https://github.com/likrot)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/likrot/react-native-ios-intents/issues"
  },
  "homepage": "https://github.com/likrot/react-native-ios-intents#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/compat": "^1.3.2",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.35.0",
    "@evilmartians/lefthook": "^1.12.3",
    "@react-native/babel-preset": "0.81.1",
    "@react-native/eslint-config": "^0.81.1",
    "@release-it/conventional-changelog": "^10.0.1",
    "@types/jest": "^29.5.14",
    "@types/react": "^19.1.0",
    "commitlint": "^19.8.1",
    "del-cli": "^6.0.0",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "jest": "^29.7.0",
    "nitrogen": "^0.31.4",
    "prettier": "^3.6.2",
    "react": "19.1.0",
    "react-native": "0.81.1",
    "react-native-builder-bob": "^0.40.13",
    "react-native-nitro-modules": "^0.31.4",
    "release-it": "^19.0.4",
    "ts-node": "^10.9.2",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*",
    "react-native-nitro-modules": "^0.31.4",
    "ts-node": ">=10.0.0",
    "typescript": ">=5.0.0"
  },
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "__fixtures__"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": {
          "name": "angular"
        }
      }
    }
  },
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "custom",
        {
          "script": "nitrogen",
          "clean": "nitrogen/"
        }
      ],
      [
        "module",
        {
          "esm": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "create-react-native-library": {
    "type": "nitro-module",
    "languages": "kotlin-swift",
    "version": "0.54.8"
  }
}
